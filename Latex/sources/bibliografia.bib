% Do dodawania kluczy do odmiennego sortowania niż według autora
% (bibtex nie radzi sobie z sortowaniem polskich znaków diakrytycznych)
@PREAMBLE{ {\providecommand{\noopsort}[1]{}} }

% Przykłady formatowania książek i artykułów w czasopismach
% Dostępne są jeszcze rozdziały w książkach (InBook),
% referaty konferencyjne (InProceedings), raporty (TechReport),
% prace nieopublikowane (Unpublished)
@book{Arn81,
    author      = "W. I. Arnold",
    title       = "Metody matematyczne mechaniki klasycznej",
    publisher   = "Państwowe Wydawnictwo Naukowe",
    year        =  1981
},

@book{TchMu18,
    author      = "K. Tchoń and R. Muszyński",
